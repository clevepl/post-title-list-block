{
	"$schema": "https://schemas.wp.org/trunk/block.json",
	"apiVersion": 2,
	"name": "cpl/post-title-list-block",
	"version": "0.1.0",
	"title": "Post Title List Block",
	"category": "widgets",
	"icon": "index-card",
	"description": "Display recently published posts' titles as a list",
	"supports": {
		"html": false,
		"customClassName": false
	},
	"attributes": {
		"numberOfPostsToDisplay": {
			"type": "number",
			"default": 5
		},
		"postTypeToObtain": {
			"type": "string"
		}
	},
	"textdomain": "post-title-list-block",
	"editorScript": "file:./index.js",
	"editorStyle": "file:./index.css",
	"style": "file:./style-index.css"
}
